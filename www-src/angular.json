{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "domoticz-frontend": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "dz",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/images",
              "src/media",
              "src/styles",
              "src/i18n",
              "src/ozwcp",
              "src/css",
              "src/css/images",
              "src/robots.txt"
            ],
            "styles": [
              "libs/bootstrap/css/bootstrap.css",
              "libs/bootstrap/css/bootstrap-responsive.css",
              "libs/jquery-ui/css/ui-darkness/jquery-ui.min.css",
              "libs/jquery-ui/css/jquery-ui-timepicker-addon.css",
              "libs/datatable/css/demo_table_jui.css",
              "libs/wheelcolorpicker.css",
              "libs/jquery.uix.multiselect.css",
              "src/styles.scss"
            ],
            "scripts": [
              "libs/jquery/jquery-3.4.1.min.js",
              "libs/jquery-ui/js/jquery-ui.min.js",
              "libs/jquery-ui/js/jquery-ui-timepicker-addon.js",
              "libs/jquery-ui/js/jquery.ui.touch-punch.min.js",
              "libs/bootstrap/js/bootstrap.min.js",
              "libs/bootbox/bootbox.min.js",
              "libs/jquery-datatables/jquery.dataTables.min.js",
              "libs/jquery-datatables/dataTables.select.min.js",
              "libs/jquery-datatables/dataTables.tableTools.min.js",
              "libs/jquery.ddslick.min.js",
              "libs/colpick.js",
              "libs/jquery.wheelcolorpicker.js",
              "libs/jquery.uix.multiselect.min.js",
              "libs/ZeroClipboard.js",
              "libs/ion/ion.sound.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "8mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "domoticz-frontend:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "domoticz-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "domoticz-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "libs/bootstrap/css/bootstrap.css",
              "libs/bootstrap/css/bootstrap-responsive.css",
              "libs/jquery-ui/css/ui-darkness/jquery-ui.min.css",
              "libs/jquery-ui/css/jquery-ui-timepicker-addon.css",
              "libs/datatable/css/demo_table_jui.css",
              "libs/wheelcolorpicker.css",
              "libs/jquery.uix.multiselect.css",
              "src/styles.scss"
            ],
            "scripts": [
              "libs/jquery/jquery-3.4.1.min.js",
              "libs/jquery-ui/js/jquery-ui.min.js",
              "libs/jquery-ui/js/jquery-ui-timepicker-addon.js",
              "libs/jquery-ui/js/jquery.ui.touch-punch.min.js",
              "libs/bootstrap/js/bootstrap.min.js",
              "libs/bootbox/bootbox.min.js",
              "libs/jquery-datatables/jquery.dataTables.min.js",
              "libs/jquery-datatables/dataTables.select.min.js",
              "libs/jquery-datatables/dataTables.tableTools.min.js",
              "libs/jquery.ddslick.min.js",
              "libs/colpick.js",
              "libs/jquery.wheelcolorpicker.js",
              "libs/jquery.uix.multiselect.min.js",
              "libs/ZeroClipboard.js",
              "libs/ion/ion.sound.min.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/images",
              "src/media",
              "src/styles",
              "src/i18n",
              "src/ozwcp",
              "src/css",
              "src/css/images",
              "src/robots.txt"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "domoticz-frontend-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "domoticz-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "domoticz-frontend:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "domoticz-frontend"
}
